@using MooMed.Web.Views
@model MooMed.Web.Models.ControllerMetaData

@{
    Layout = "~/Views/Shared/Other/_OtherLayout.cshtml";
}

<script>
    @if (Model.ReactRoute != null)
    {
        @:window.reactRoute = "@Html.Raw(Model.ReactRoute)";
    }

    @if (Model.DataModel != null)
    {
        @:window.dataModel = JSON.parse('@Html.Raw(Model.DataModel)');
    }
</script>

<div id="otherPageHolder" class="otherPageHolder"></div>

@MooMedHtmlHelper.TranslationFile(Model.UiLanguage);