{
  "compilerOptions": {
    "baseUrl": "React",
    "removeComments": true,
    "sourceMap": true,
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
		// "noImplicitAny": true,
		// "noUnusedLocals": true,
		// "noUnusedParameters": true,
    "allowJs": false,
    "paths": {
      "components": ["Views/Components/*"],
      "data": ["Data/*"],
      "definitions": ["Definitions/*"],
      "helper": ["Helper/*"],
      "hooks": ["Hooks/*"],
      "modules": ["Modules/*"],
      "utils": ["Helper/Utils/*"],
      "views": [ "Views/*"],
	  "enums":  ["Enums/*"]
    }
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
